<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.2</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('settings')]" position="inside">
                    <div class="app_settings_block" data-string="Connector" string="Connector" data-key="connect" >
                        <h2>18 Connector</h2>
                        <div class="row mt16 o_settings_container" name="18_connector_setting_container">
                            <div class="col-12 col-lg-6 o_setting_box">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                    <div class="row mt16">
                                        <label for="connector_base_url" string="Url" class="col-2 o_light_label"/>
                                        <field name="connector_base_url" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-lg-6 o_setting_box">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                    <div class="row mt16">
                                        <label for="connector_email" string="Email" class="col-2 o_light_label"/>
                                        <field name="connector_email" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-lg-6 o_setting_box">
                                <div class="o_setting_left_pane"/>
                                <div class="o_setting_right_pane">
                                    <div class="row mt16">
                                        <label for="connector_pwd" string="Password" class="col-2 o_light_label"/>
                                        <field name="connector_pwd" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
</odoo>